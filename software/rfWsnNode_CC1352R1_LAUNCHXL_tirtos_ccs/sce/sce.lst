0021 ---- 68ad                             ld          R6, [#fwCtrlExt/bvTaskTerminateReq]
0022 ---- 00a1                             ld          R0, #pFwTaskTerminateFuncTable
0023 ---- 1431                             jsr         /handleReq
               

004f ---- 0ca2                                     st          R0, [#fwCtrlInt/taskId]
0050 8600 88a0                                     add         R0, #pFwTaskExecuteFuncTable
0052 ---- 8f08                                     ld          R0, [R0]
                                                   ; Insert NOP to avoid pipeline hazard
0053 ---- fd47                                     nop
0054 ---- 9db7                                     jsr         R0
0055 ---- 08a2                                     ld          R0, [#fwCtrlInt/taskId]
               /skip:

0074 ---- 0000                         ld          R0, #0
0075 ---- 0ca2                         st          R0, [#fwCtrlInt/taskId]
0076 ---- 0004                         ld          R0, #(1 << 2)
0077 ---- 0ca3                         st          R0, [#fwCtrlInt/bvActiveEvent]

